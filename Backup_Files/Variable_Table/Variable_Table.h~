#ifndef VARIABLETABLE_H
#define VARIABLETABLE_H
#include <iosfwd>
#include <string>
#include <stdexcept>
#include <map>

using var_map = map<string,long double>;

class Variable_Table
{
public:
    ~Variable_Table() = default;
    Variable_Table() = default;
    void insert(const std::string&, long double);

private:
    var_map variable_table;
}
















#endif
